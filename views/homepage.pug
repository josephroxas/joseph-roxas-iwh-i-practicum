doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Custom Object Table
        a(href="/update-cobj") Add/Update this table
        table
            tr
                th Name
                th Model
                th Year
            each car in data
                tr
                    td #{car.properties.name}
                    td #{car.properties.model}
                    td #{car.properties.year}